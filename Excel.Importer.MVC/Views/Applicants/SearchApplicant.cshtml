@model List<Applicant>

<h2>Applicants</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Group Name</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var applicant in Model)
        {
            <tr>
                <td>@applicant.FirstName</td>
                <td>@applicant.LastName</td>
                <td>@applicant.Email</td>
                <td>@applicant.PhoneNumber</td>
                <td>@applicant.GroupName</td>
            </tr>
        }
    </tbody>
</table>