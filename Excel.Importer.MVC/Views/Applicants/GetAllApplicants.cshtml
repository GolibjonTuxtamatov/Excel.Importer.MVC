@model IQueryable<Applicant>

<h1>Applicants</h1><br />
<a asp-action="PostApplicant" class="btn btn-outline-primary">
    Add Applicant
</a>

<table class="table">
    <thead>
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
            <th>PhoneNumber</th>
        </tr>
    </thead>
    <tbody class="table-applicant-driver">
        @foreach (var applicant in Model)
        {
            <tr>
                <td>@applicant.FirstName</td>
                <td>@applicant.LastName</td>
                <td>@applicant.Email</td>
                <td>@applicant.PhoneNumber</td>
                <td>@applicant.GroupName</td>
            </tr>
        }
    </tbody>
</table>
