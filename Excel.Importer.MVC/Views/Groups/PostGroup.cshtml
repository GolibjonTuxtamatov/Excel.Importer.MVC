@model Excel.Importer.MVC.Models.Foundations.Groups.Group

@{
    ViewData["Title"] = "PostGroup";
}

<h4>Group</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post" asp-action="PostGroup">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="GroupName" class="control-label"></label>
                <input asp-for="GroupName" class="form-control" />
                <span asp-validation-for="GroupName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-outline-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
